	<parameter name="codigo_veiculo" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[SELECT veiculo.codigo,
	veiculo.anofabricacao,
	veiculo.chassi,
	veiculo.chave,
	veiculo.obs,
	veiculo.placa,
	veiculo.placaoriginal,
	cor.cor,
	marca.marca,
	modelo.modelo,
	ocorrencia.data,
	ocorrencia.ocorrencia
FROM ocorrencia
	INNER JOIN veiculo ON 
	 ocorrencia.codigo_veiculo = veiculo.codigo 
	INNER JOIN cor ON 
	 cor.codigo = veiculo.codigo_cor 
	INNER JOIN marca ON 
	 marca.codigo = veiculo.codigo_marca 
	INNER JOIN modelo ON 
	 modelo.codigo = veiculo.codigo_modelo 
WHERE veiculo.codigo = 	 $P!{codigo_veiculo}]]>
	</queryString>
